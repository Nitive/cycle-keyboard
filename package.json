    "keypress",
    "keydown",
    "keyup",
    "cycle"
  ],
  "author": "Sudarsan Balaji",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artfuldev/cycle-keyboard/issues"
  },
  "homepage": "https://github.com/artfuldev/cycle-keyboard",
  "dependencies": {
    "keycode": "^2.1.2",
    "xstream": ">= 5.1.0"
  },
  "devDependencies": {
    "@types/jest": "^16.0.1",
    "jest": "^17.0.3",
    "ts-jest": "^17.0.3",
    "typescript": "^2.0.10",
    "typings": "^1.3.1"
  },
  "jest": {
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
